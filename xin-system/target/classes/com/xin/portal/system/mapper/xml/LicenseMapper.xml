<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xin.portal.system.mapper.LicenseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xin.portal.system.model.License">
        <id column="id" property="id" />
        <result column="apply_time" property="applyTime" />
        <result column="apply_type" property="applyType" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="type" property="type" />
        <result column="amount" property="amount" />
        <result column="ext_info" property="extInfo" />
        <result column="state" property="state" />
        <result column="creater" property="creater" />
        <result column="company" property="company" />
        <result column="email" property="email" />
        <result column="phone" property="phone" />
        <result column="address" property="address" />
        <result column="approver" property="approver" />
        <result column="approve_time" property="approveTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, apply_time AS applyTime, apply_type AS applyType, start_time AS startTime, end_time AS endTime, type, amount, ext_info AS extInfo, state, creater, approver, approve_time AS approveTime
    </sql>
</mapper>
