<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xin.portal.system.mapper.SysReleaseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xin.portal.system.model.SysRelease">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="publisher" property="publisher" />
        <result column="release_time" property="releaseTime" />
        <result column="release_version" property="releaseVersion" />
        <result column="content" property="content" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, publisher, release_time, release_version, content
    </sql>

</mapper>
